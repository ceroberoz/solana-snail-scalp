# OANDA API Configuration
# Copy this file to .env and fill in your credentials
# DO NOT COMMIT .env TO GIT!

# Get your API key from: https://www.oanda.com/demo-account/
# Account ID is in your OANDA dashboard (format: xxx-xxx-xxx-xxx)

OANDA_API_KEY=your-api-key-here
OANDA_ACCOUNT_ID=your-account-id-here

# Environment: "practice" (demo) or "live" (real money)
OANDA_ENVIRONMENT=practice
